{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"globalDotEnv": [".env.local"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"]
		},
		"lint": {},
		"dev": {
			"dotEnv": [".env.development.local"],
			"env": ["TRANSIP_PKEY", "TRANSIP_LOGIN"],
			"cache": false,
			"persistent": true
		},
		"test": {}
	}
}
